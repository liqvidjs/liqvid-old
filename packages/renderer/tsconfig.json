{
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": false,
    "esModuleInterop": true,
    "incremental": true,
    "jsx": "react",
    "lib": ["esnext", "dom"],
    "moduleResolution": "node",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./dist",
    "pretty": true,
    "removeComments": true,
